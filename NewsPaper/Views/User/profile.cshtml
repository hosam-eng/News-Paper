@model List<NewsPaper.Models.News>
@{
    ViewBag.Title = "profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.ActionLink("Edit Profile", "edit", new { id = ViewBag.id }, new { @class = "btn btn-primary" });
@Html.ActionLink("Create News", "create", "News", null, new { @class = "btn btn-primary" });
@Html.ActionLink("All News ", "show_news", "News", null, new { @class = "btn btn-primary" });
<table class="table table-active">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.newsID</td>
            <td>@item.title</td>
            <td>@item.description</td>
            <td>@item._ref</td>
            <td>@item.datehire</td>
            <td><img src="~/attach/@item.photo" width="100" height="100" /></td>
            <td>@Html.ActionLink("Edit", "edit_news", "News", new { id = item.newsID }, new { @class = "btn btn-primary" })</td>
        </tr>
    }
</table>

